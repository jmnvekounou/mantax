{% extends 'base.html.twig' %}

{% block title %}Income{% endblock %}

{% block body %}
    <h1>Income</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ income.id }}</td>
            </tr>
            <tr>
                <th>Billnumber</th>
                <td>{{ income.billnumber }}</td>
            </tr>
            <tr>
                <th>Amount</th>
                <td>{{ income.amount }}</td>
            </tr>
            <tr>
                <th>Tps</th>
                <td>{{ income.tps }}</td>
            </tr>
            <tr>
                <th>Tvq</th>
                <td>{{ income.tvq }}</td>
            </tr>
            <tr>
                <th>Tvh</th>
                <td>{{ income.tvh }}</td>
            </tr>
            <tr>
                <th>Total</th>
                <td>{{ income.total }}</td>
            </tr>
            <tr>
                <th>ReceivedAt</th>
                <td>{{ income.receivedAt ? income.receivedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ income.createdAt ? income.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('income_index') }}">back to list</a>

    <a href="{{ path('income_edit', {'id': income.id}) }}">edit</a>

    {{ include('income/_delete_form.html.twig') }}
{% endblock %}
